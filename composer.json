{
    "name": "spatie/laravel-dashboard-oh-dear-uptime-tile",
    "description": "A uptime tile for Laravel Dashboard",
    "keywords": [
        "spatie",
        "laravel-dashboard-oh-dear-uptime-tile"
    ],
    "homepage": "https://github.com/spatie/laravel-dashboard-oh-dear-uptime-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ohdearapp/laravel-ohdear-webhooks": "^1.3",
        "spatie/laravel-dashboard": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\OhDearUptimeTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\OhDearUptimeTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "\\Spatie\\OhDearUptimeTile\\OhDearUptimeTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
